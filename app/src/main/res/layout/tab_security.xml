<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".security">


    <!-- TODO: Update blank fragment layout -->

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background">

        <CheckBox
            android:id="@+id/checkBox3"
            android:layout_width="115dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:checked="true"
            android:clickable="false"
            android:fontFamily="sans-serif-medium"
            android:text="@string/allowed"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="290dp"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif"
            android:text="Geolocation / Current Location"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/locationCheckBox"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="290dp"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif"
            android:text="Video/Audio Files"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/videoAudioFilesCheckBox"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="290dp"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif"
            android:text="Social Security Number"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/socialSecurityCheckBox"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="290dp"
            android:layout_height="31dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif"
            android:text="Email Address"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/emailAddressCheckBox"
            app:layout_constraintStart_toStartOf="parent" />

        <CheckBox
            android:id="@+id/userNameCheckBox"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_alignStart="@+id/button4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fullNameCheckBox" />

        <CheckBox
            android:id="@+id/emailAddressCheckBox"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_alignStart="@+id/button4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/telephoneCheckBox" />

        <CheckBox
            android:id="@+id/telephoneCheckBox"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_alignStart="@+id/button4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/homeAddressCheckBox" />

        <CheckBox
            android:id="@+id/videoAudioFilesCheckBox"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_alignBottom="@+id/textView4"
            android:layout_alignStart="@+id/button4"
            android:layout_marginEnd="25dp"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/locationCheckBox" />

        <CheckBox
            android:id="@+id/locationCheckBox"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_alignStart="@+id/button4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/socialSecurityCheckBox" />

        <CheckBox
            android:id="@+id/socialSecurityCheckBox"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_alignStart="@+id/button4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/emailAddressCheckBox" />

        <CheckBox
            android:id="@+id/fullNameCheckBox"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_alignStart="@+id/button4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/checkBox3" />

        <CheckBox
            android:id="@+id/homeAddressCheckBox"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_alignStart="@+id/button4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/userNameCheckBox" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="290dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif"
            android:text="Full Name"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/fullNameCheckBox"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="290dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif"
            android:text="Home Address"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/homeAddressCheckBox"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="290dp"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif"
            android:text="Telephone / Mobile Number"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/telephoneCheckBox"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="290dp"
            android:layout_height="30dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif"
            android:text="UserName"
            android:textColor="#000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/userNameCheckBox"
            app:layout_constraintStart_toStartOf="parent" />

    </android.support.constraint.ConstraintLayout>

</RelativeLayout>